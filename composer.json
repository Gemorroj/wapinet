{
    "name": "gemorroj/wapinet.ru",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": "~5.6",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "javiereguiluz/easyadmin-bundle": "~1.11",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "friendsofsymfony/comment-bundle": "~2.0@dev",
        "friendsofsymfony/message-bundle": "dev-master",
        "white-october/pagerfanta-bundle": "~1.0",
        "james-heinrich/getid3": "dev-master",
        "vich/uploader-bundle": "~1.0@dev",
        "liip/imagine-bundle": "~1.5",
        "gregwar/captcha-bundle": "~1.1",
        "sujayjaju/ffmpeg-bundle": "dev-master",
        "highco/sphinx-bundle": "~2.0",
        "tufanbarisyildirim/php-apk-parser": "dev-master",
        "geoip2/geoip2": "~2.3",
        "christeredvartsen/php-bittorrent": "~1.1",
        "cypresslab/gitelephant": "~1.1",
        "simple-updates/phpwhois": "dev-master",
        "gemorroj/archive7z": "dev-master",
        "gemorroj/xbbcode": "dev-master",
        "gemorroj/syntax": "dev-master",
        "gemorroj/htmlvalidator": "dev-master",
        "gemorroj/cssvalidator": "dev-master",
        "gemorroj/m3u-parser": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "phpunit/phpunit": "~4.8"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
