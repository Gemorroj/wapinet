{
    "name": "gemorroj/wapinet",
    "description": "wapinet.ru website",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "ext-json": "*",
        "ext-pdo_mysql": "*",
        "ext-intl": "*",
        "ext-apcu": "*",
        "ext-simplexml": "*",
        "ext-sodium": "*",
        "ext-gd": "*",

        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17.1",
        "beberlei/doctrineextensions": "dev-master",

        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24",
        "symfony/asset": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/yaml": "6.3.*",
        "symfony/webpack-encore-bundle": "^2.1",

        "twig/extra-bundle": "^3.8",
        "twig/twig": "^3.8",

        "babdev/pagerfanta-bundle": "^4.4",
        "pagerfanta/doctrine-collections-adapter": "^v4.2",
        "pagerfanta/doctrine-orm-adapter": "^v4.2",
        "pagerfanta/twig": "^v4.2",

        "easycorp/easyadmin-bundle": "^4.8",
        "friendsofsymfony/jsrouting-bundle": "^3.4",
        "james-heinrich/getid3": "^1.9.23",
        "vich/uploader-bundle": "^2.3",
        "liip/imagine-bundle": "^2.12",
        "gregwar/captcha-bundle": "^2.2",
        "tufanbarisyildirim/php-apk-parser": "^3.0",
        "geoip2/geoip2": "^3.0",
        "christeredvartsen/php-bittorrent": "^2.0",
        "foolz/sphinxql-query-builder": "^3.0",
        "php-ffmpeg/php-ffmpeg": "^1.1",
        "riverline/multipart-parser": "^2.1",
        "stdtabs/phptabs": "^1.1.0",

        "gemorroj/phpwhois": "dev-master",
        "gemorroj/stop-spam": "^3.4",
        "gemorroj/archive7z": "^5.7",
        "gemorroj/m3u-parser": "^5.5",
        "gemorroj/syntax": "^4.4",
        "gemorroj/htmlvalidator": "^3.3",
        "gemorroj/cssvalidator": "^3.4",
        "gemorroj/xbbcode": "^2.7",
        "gemorroj/ginfo": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "^1.52",
        "symfony/phpunit-bridge": "6.3.*",
        "symfony/stopwatch": "6.3.*",
        "symfony/var-dumper": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-doctrine": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.41"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@php bin/anti-terrorism"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@php bin/anti-terrorism"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    }
}
