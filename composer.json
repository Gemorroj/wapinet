{
    "name": "gemorroj/wapinet",
    "description": "wapinet.ru website",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "ext-json": "*",
        "ext-apcu": "*",
        "ext-simplexml": "*",
        "sensio/framework-extra-bundle": "^5.3",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.2",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "*",
        "symfony/process": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/yaml": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "easycorp/easyadmin-bundle": "^2.2",
        "friendsofsymfony/user-bundle": "^2.1@dev",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "white-october/pagerfanta-bundle": "^1.2",
        "james-heinrich/getid3": "^1.9.17",
        "vich/uploader-bundle": "^1.9",
        "liip/imagine-bundle": "^2.1",
        "gregwar/captcha-bundle": "^2.0",
        "tufanbarisyildirim/php-apk-parser": "dev-master",
        "geoip2/geoip2": "^2.9",
        "christeredvartsen/php-bittorrent": "^1.1",
        "seostats/seostats": "^2.5@dev",
        "foolz/sphinxql-query-builder": "^2.0",
        "php-ffmpeg/php-ffmpeg": "^0.13",
        "riverline/multipart-parser": "^1.2",
        "gemorroj/phpwhois": "dev-master",
        "gemorroj/stop-spam": "^3.0",
        "gemorroj/archive7z": "^4.0",
        "gemorroj/m3u-parser": "^4.0",
        "gemorroj/syntax": "^3.0",
        "gemorroj/htmlvalidator": "^1.1",
        "gemorroj/cssvalidator": "^1.1",
        "gemorroj/xbbcode": "^2.3",
        "gemorroj/ginfo": "dev-master"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*",
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan-symfony": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
