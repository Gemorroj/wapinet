{% extends "::base.html.twig" %}

{% block title %}Sony Ericsson{% endblock %}
{% block breadcrumbs %}
    {{wapinet_breadcrumbs({
    1: {'uri': path('utilities'), 'title': 'Утилиты'},
    2: {'uri': path('mobile_code_index'), 'title': 'Телефонные коды'}
    })}}
{% endblock %}

{% block content %}
    <div class="ui-corner-all custom-corners">
        <div class="ui-bar ui-bar-a">
            <h3>Нетмонитор в новых моделях</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>*585*0000#</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Переключение на англ яз</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>*#0000#</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Меню без SIM</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>**04*0000*0000*0000#</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Меню сети</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>&lt;**&lt;</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Внутрителефонное меню</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>&gt;*&lt;&lt;*&lt;*</p>
        </div>
    </div>
{% endblock %}
