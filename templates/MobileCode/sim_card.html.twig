{% extends "base.html.twig" %}

{% block title %}Сим карты{% endblock %}
{% block breadcrumbs %}
    {{wapinet_breadcrumbs({
    1: {'uri': path('mobile_code_index'), 'title': 'Телефонные коды'}
    })}}
{% endblock %}

{% block content %}
    <div class="ui-corner-all custom-corners">
        <div class="ui-bar ui-bar-a">
            <h3>Изменить PIN</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>**04*oldPIN*newPIN*newPIN# кнопка посыла вызова</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Изменить PIN2</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>**042*oldPIN2*newPIN2*newPIN2# кнопка посыла вызова</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Разблокировка PIN</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>**05*PUK*newPIN*newPIN# кнопка посыла вызова</p>
        </div>
        <div class="ui-bar ui-bar-a">
            <h3>Разблокировка PIN2</h3>
        </div>
        <div class="ui-body ui-body-a">
            <p>**052*PUK2*newPIN2*newPIN2# кнопка посыла вызова</p>
        </div>
    </div>
{% endblock %}
